<template name="quickcancel">
  <view class="detail-content" wx:if="{{show}}">
    <view class="det-content">{{content}}</view>
    <view class="flex-box">
      <view bindtap="buyIt" class="flex2 det-btn-buy" data-type="{{type}}">选择购买</view>
      <view class="flex1"></view>
      <view bindtap="buyMao" class="flex2 det-btn-nobuy" data-index="{{index||0}}" data-type="{{type}}">坚决不买</view>
    </view>
  </view>
</template>
<template name="valueAddService">
  <view bindtap="btnOpenOpt" class="ser-ins flex-box optimization" style="order:{};" wx:if="{{!pkg.isValue&&plists.length&&(isHasOptSvc||isHasExtSvc)}}">
    <block wx:if="{{isAdult&&isHasOptSvc}}">
      <text class="ser-instext">优选服务</text>
      <text class="ser-arrow flex1" wx:if="{{defaultOpt.sellPrice==0}}">不购买服务</text>
      <text class="ser-arrow flex1" wx:else>{{defaultOpt.subTitle}}</text>
    </block>
    <block wx:if="{{!isAdult&&isHasExtSvc}}">
      <text class="ser-instext">至尊服务</text>
      <text class="ser-arrow flex1" wx:if="{{defaultExt.sellPrice==0}}">不购买服务</text>
      <text class="ser-arrow flex1" wx:else>{{defaultExt.subTitle}}</text>
    </block>
  </view>
</template>
<form bindsubmit="formBookingBtn" reportSubmit="{{true}}">
  <label>
    <button formType="submit" hidden="{{true}}" type="primary"></button>
    <view class="main" style="">
      <view class="top">
        <view class="topInfo">
          <view class="top-box" wx:if="{{isSecStyHeader=='B'}}">
            <view class="top-date">{{busInfo.formatDate}}({{busInfo.week}})
              <block wx:if="{{busInfo.runningSchFlag==1&&busInfo.bookingType==1}}">
                <block wx:if="{{busInfo.runningStartTime&&busInfo.runningEndTime}}">
                  <strong>{{busInfo.runningStartTime}}-{{busInfo.runningEndTime}}</strong>发车</block>
                <block wx:elif="{{busInfo.runningStartTime&&!busInfo.runningEndTime}}">
                  <strong>{{busInfo.runningStartTime}}</strong>后发车</block>
                <block wx:elif="{{!busInfo.runningStartTime&&busInfo.runningEndTime}}">
                  <strong>{{busInfo.runningEndTime}}</strong>前发车</block>
              </block>
              <block wx:else>
                <strong>{{busInfo.dptTime}}</strong>发车</block>
            </view>
            <view class="top-detail">
              <view class="top-line top-start flex-box flex-box-center">
                <view class="flex1">{{busInfo.dptStation}}</view>
              </view>
              <view class="top-line top-end flex-box flex-box-center">
                <view class="flex1">{{busInfo.arrStation}}</view>
                <view class="top-state ticketType">{{busInfo.coachType?busInfo.coachType:''}}{{busInfo.coachType&&busInfo.scheduleTicketType?'|':''}}
                  <text bindtap="btnToggleTicketTypePop" data-toggle="1">{{busInfo.scheduleTicketType?busInfo.scheduleTicketType:''}}</text>
                  <text bindtap="btnToggleTicketTypePop" class="pp" data-toggle="0" wx:if="{{isShowTicketTypeTips}}">{{busInfo.STTDesc}}</text>
                </view>
              </view>
            </view>
          </view>
          <view class="top-boxs" wx:elif="{{isSecStyHeader=='A'}}">
            <view class="bussche-msg flex-box">
              <view class="flex1 port-msg left-port-msg">
                <view class="station-msg">{{busInfo.dptStation}}</view>
                <view class="city-msg">{{busInfo.departure}}</view>
              </view>
              <view class="bussche-time">
                <view class="bussche-clock">{{busInfo.dptTime}}</view>
                <view class="bussche-decoration"></view>
                <view class="bussche-date">{{busInfo.formatDate}}
                  <text></text>{{busInfo.week}}</view>
              </view>
              <view class="flex1 port-msg right-port-msg">
                <view class="station-msg">{{busInfo.arrStation}}</view>
                <view class="city-msg">{{busInfo.destination}}</view>
              </view>
            </view>
            <view class="sche-btline flex-box">
              <view class="flex1">
                <text class="sche-coach">{{busInfo.coachType?busInfo.coachType:''}}</text>
                <text catchtap="showService" class="sche-coach" data-type="Ttype" wx:if="{{busInfo.scheduleTicketType}}">{{busInfo.scheduleTicketType}}</text>
              </view>
              <view class="flex1 sche-right">
                <view class="tick-price flex-box">
                  <view>票价￥{{busInfo.ticketPrice}}</view>
                  <view bindtap="showService" data-type="service" wx:if="{{ChargeFee>0}}">
                    <text>服务费￥{{ChargeFee}}</text>
                    <text class="ticket-service-icon">i</text>
                  </view>
                </view>
              </view>
            </view>
          </view>
          <view bindtap="btnOpenNotice" class="{{isSecStyHeader=='A'?'top-points':isSecStyHeader=='B'?'top-point':'top-point'}} top-point flex-box flex-box-center">
            <view class="flex1" style="color:#11c9a1;font-size:30rpx" wx:if="{{isTodaySch&&busInfo.isRefund}}">今天{{refundTime1}}前可网上退票</view>
            <view class="flex1" style="color:#11c9a1;font-size:30rpx" wx:elif="{{busInfo.isRefund&&!isTodaySch}}">发车前{{refundTime1}}可在线退票</view>
            <view class="flex1" style="font-size:30rpx" wx:else>发车前至发车站窗口退票</view>
            <view class="ser-arrow">取票、退票详情</view>
          </view>
        </view>
        <view bindtap="btnToggleAlter" class="ser-ins flex-box bor-top" wx:if="{{childPriceCount<=0&&isSimpleBeixuan=='B'}}">
          <text class="ser-instext">备选车次</text>
          <text class="ser-arrow flex1">{{alterTxt}}</text>
        </view>
        <view bindtap="btnToggleSimAlter" class="ser-ins flex-box bor-top" wx:if="{{childPriceCount<=0&&isSimpleBeixuan=='A'}}">
          <text class="ser-instext small-beixuan">无票时更换为{{Simhour}}的班次</text>
          <view class="flex1">
            <view class="ricon small-radio {{isSelSimpleBeixuan?'ricon-check':''}}"></view>
          </view>
        </view>
        <view bindtap="btnToggleSubscribe" class="ser-ins flex-box bor-top" wx:if="{{vipSubscribe&&vipSubscribePrice&&!ASisExceedLimit}}">
          <text class="ser-instext">{{subscribeInfoObj.title}}</text>
          <text class="ser-arrow flex1">{{subscribeInfoObj.subTitle}}</text>
        </view>
      </view>
      <view class="pass">
        <view class="pass-cap flex-box flex-box-center bor-bottom" wx:if="{{plists.length||fillPassList.length}}">
          <view class="pass-title">乘客</view>
          <view>最多添加{{maxNum}}人</view>
        </view>
        <view>
          <view wx:for="{{plists}}" wx:key="id">
            <view class="pass-box flex-box bor-bottom selected-item">
              <view bindtap="fnShowDeleteBtn" class="pass-delete {{item.showDeleteBtn?'show-delete':''}}" data-deltype="selected" data-pid="{{item.IDSpace}}" data-pname="{{item.linkerName}}" data-serial="{{index}}"></view>
              <view class="flex1">
                <view class="pass-name">{{item.linkerName}}
                  <text class="child-icon" wx:if="{{isSupportChild&&item.type==1}}">儿童</text>
                </view>
                <view class="pass-card">{{item.IDSpace}}</view>
              </view>
              <view bindtap="btnEditPass" class="edit-icon" data-from="notpop" data-pid="{{item.IDCard}}" data-pidspace="{{item.IDSpace}}" data-pname="{{item.linkerName}}" data-type="{{item.type}}" wx:if="{{item.source!=1}}"></view>
              <button bindtap="btnDeletePass" class="{{item.showDeleteBtn?'show':''}}" data-pid="{{item.IDCard}}" data-pidspace="{{item.IDSpace}}" data-pname="{{item.linkerName}}" data-serial="{{index}}">删除</button>
            </view>
            <block wx:if="{{supportFreeChild}}">
              <view class="freeChildItem pass-box flex-box bor-bottom selected-item" wx:for="{{item.carryChildren}}" wx:for-index="idx" wx:for-item="one">
                <view bindtap="fnShowFCDeleteBtn" class="pass-delete {{one.showDeleteBtn?'show-delete':''}}" data-idx="{{idx}}" data-pid="{{item.IDCard}}" data-pname="{{item.linkerName}}"></view>
                <view class="flex1">
                  <view class="pass-name">{{one.name}}
                    <text class="child-icon">携童</text>
                  </view>
                  <view class="pass-card">{{one.idSpace}}</view>
                </view>
                <view bindtap="btnEditFreeChild" class="edit-icon" data-idx="{{idx}}" data-pid="{{item.IDCard}}" data-pname="{{item.linkerName}}"></view>
                <button bindtap="btnDeleteFreeChild" class="{{one.showDeleteBtn?'show':''}}" data-idx="{{idx}}" data-pid="{{item.IDCard}}" data-pname="{{item.linkerName}}">删除</button>
              </view>
            </block>
          </view>
        </view>
        <view class="addbtn-wrap flex-box flex-box-center flex-box-justify">
          <view bindtap="btnOpenPass" class="pass-addbtn flex1" wx:if="{{fillPassList.length||passengers.length}}">
            <text class="pass-addtxt">添加乘客</text>
          </view>
          <view bindtap="btnShowAddPage" class="pass-addbtn flex1" wx:else>
            <text class="pass-addtxt">添加乘客</text>
          </view>
        </view>
      </view>
      <view bindtap="btnAddFreeChild" class="t-radius b-radius ser-arrow ser-ins freeChild" wx:if="{{supportFreeChild&&(localInfo.freeChildCntLeft-adultCount>=0?adultCount:localInfo.freeChildCntLeft)-freeChildCount>0&&localInfo.freeChildCntLeft>0}}">免费申请携童票
        <text>1.2米以下儿童/无座位</text>
      </view>
      <view class="take" wx:if="{{adultPriceCount}}">
        <view class="take-box">
          <view class="take-cap flex-box bor-bottom">
            <view class="take-title flex1">取票人
              <text class="take-name">{{contactInfo.linkerName}}</text>
            </view>
            <view bindtap="btnShowContact" class="take-arrow">
              <text wx:if="{{isHiddenContact}}">修改</text>
              <text wx:else>收起</text>
            </view>
          </view>
          <view hidden="{{isHiddenContact}}">
            <view class="take-main flex-box bor-bottom">
              <view bindtap="btnSelectContact" class="take-list{{item.IDCard==contactInfo.IDCard?' take-selected':''}}" data-cid="{{item.IDCard}}" data-cname="{{item.linkerName}}" wx:if="{{isSupportChild&&item.type!=1||!isSupportChild}}" wx:for="{{plists}}" wx:key="id">{{item.linkerName}}</view>
            </view>
            <view class="other"></view>
          </view>
        </view>
        <view class="phone flex-box">
          <view class="phone-title">手机号</view>
          <input bindblur="blurPhone" bindinput="inputPhoneNum" class="phone-input flex1" focus="{{phoneFocus}}" maxlength="13" placeholder="用于接收取票信息" placeholderClass="placeholder" type="number" value="{{phoneSpace}}"></input>
          <view class="phone-close"></view>
        </view>
      </view>
      <view bindtap="btnOpenCars" class="ser-ins flex-box hasCoupons" wx:if="{{plists.length>0&&!!isGetCard&&cardMsg.canUseNumber>0&&highPriceCoupon>0}}">
        <view class="ser-instext flex_1">优惠券</view>
        <view class="ser-arrow flex_2 colorful">
          <block wx:if="{{selectCode.showPrice>0}}">已优惠
            <text class="red-word">¥{{selectCode.showPrice}}</text>
          </block>
          <block wx:else>
            <text class="card-bg">{{cardMsg.canUseNumber}}张</text>最高可减
            <text class="red-word">¥{{highPriceCoupon}}</text>
          </block>
        </view>
      </view>
      <view style="display:flex;flex-direction:column">
        <view class="revenue" style="order:{};" wx:if="{{stList.length}}">
          <block wx:for="{{stList}}" wx:for-index="idx" wx:key="code">
            <view bindtap="selectService" class="rbox flex-box flex-box-center" data-key="{{item.statusKey}}" data-rmb="{{item.price||0}}" hidden="{{item.code=='refundCarefree'&&!isShowSuixin||item.code=='homeDelivery'&&localInfo.revenueType==2}}" wx:if="{{item.code&&plists.length&&item.code!='FastTicketOut'&&item.code!='refundCarefree'||item.code=='homeDelivery'&&plists.length||item.code=='FastTicketOut'&&plists.length&&isShowSpeed||item.code=='refundCarefree'&&plists.length&&isShowrefund}}">
              <view class="icon-yymd">
                <image src="{{item.img}}"></image>
              </view>
              <view class="flex1">
                <view class="rtitle" wx:if="{{item.code=='FastTicketOut'}}">{{item.name}}
                  <text catchtap="btnOpenPop" class="rdetail hottips"></text>
                  <text class="rdetailTop" wx:if="{{item.tag}}">{{item.tag}}</text>
                </view>
                <view class="rtitle" wx:if="{{item.code=='refundCarefree'}}">{{item.name}}
                  <text catchtap="btnRefundPop" class="rdetail"></text>
                  <text class="rdetailTop" wx:if="{{item.tag}}">{{item.tag}}</text>
                </view>
                <view class="rtitle" wx:if="{{item.code=='homeDelivery'}}">{{item.name}}
                  <text class="rdetailTop" wx:if="{{item.tag}}">{{item.tag}}</text>
                </view>
                <view class="rtips">{{item.description}}</view>
              </view>
              <view class="new-reprice new-fbox" wx:if="{{item.code!='homeDelivery'}}">¥{{item.price}}/份</view>
              <view class="ricon {{YyStatus?'ricon-check':''}}" wx:if="{{item.code=='BusYiyuanFree'}}"></view>
              <view class="ricon {{IssueStatus?'ricon-check':''}}" wx:if="{{item.code=='FastTicketOut'}}"></view>
              <view class="ricon {{refundStatus?'ricon-check':''}}" wx:if="{{item.code=='refundCarefree'}}"></view>
              <view class="ricon {{homeDeliveryStatus?'ricon-check':''}}" wx:if="{{item.code=='homeDelivery'}}"></view>
            </view>
            <view class="sendbox" hidden="{{!homeDeliveryStatus}}" wx:if="{{item.code=='homeDelivery'&&plists.length}}">
              <view bindtap="sendDoorAdd" class="sbox mail arrow new-fbox bor-bottom">
                <view class="bymail">邮寄地址</view>
                <view class="new-fbox mian-addBox" style="width:70%">
                  <text class="addadd" wx:if="{{!hasAddress}}">请添加邮寄地址</text>
                  <view class="address-box" wx:elif="{{hasAddress}}">
                    <view class="name-wrap">
                      <text class="name-box">{{addressInfo.addName}}</text>
                      <text class="phone-box">{{addressInfo.contactPhone}}</text>
                    </view>
                    <view class="region-box">{{addressInfo.region}}{{addressInfo.addressDetail}}</view>
                  </view>
                </view>
              </view>
              <view bindtap="sfText" class="sfbox flex-box flex-box-center bor-bottom">
                <view class="flex1">
                  <view class="rtitle">顺丰保价
                    <text bindtap="sfDetail" class="rdetail" data-ids="sfbj"></text>
                  </view>
                  <view class="rtips">快递车票丢失、延误，全额赔付</view>
                </view>
                <view class="new-reprice font-red">免费赠送</view>
                <view class="ricon {{sfText?'ricon-check':''}}"></view>
              </view>
              <view bindtap="sendNoticeShow" class="sbox notice arrow">送票上门须知</view>
            </view>
          </block>
          <view bindtap="busVoucherSel" class="rbox flex-box flex-box-center" wx:if="{{plists.length&&busVoucher.vouShowPrice>0}}">
            <view class="icon-yymd">
              <image src="http://file.40017.cn/img140017cnproduct/touch/bus/wx_bus/xgift.png"></image>
            </view>
            <view class="flex1">
              <view class="rtitle">{{busVoucher.vouSellPrice}}元汽车票礼包
                <text catchtap="busVoucherPop" class="rdetail"></text>
                <text class="rdetailTop">促销</text>
              </view>
              <view class="rtips">购汽车票享立减，最高{{busVoucher.vouShowPrice}}元</view>
            </view>
            <view class="ricon {{busVoucherIsSel?'ricon-check':''}}"></view>
          </view>
        </view>
        <view bindtap="selectPrefer" class="ser-ins flex-box bor-top reducePrefer" wx:if="{{havePerferInfo}}">
          <text class="ser-instext">立减优惠</text>
          <view class="RMBswitch">
            <view class="price">
              <text>-¥{{preferInfo.preferentialType}}</text>/{{!!preferInfo.preferentialDimension?'人':'单'}}</view>
            <view class="ricon {{selectedPrefer?'ricon-check':''}}"></view>
          </view>
        </view>
        <view class="flex-box bor-top item-div jiudianbox" style="order:{};" wx:if="{{hasHotelCoupon&&plists.length}}">
          <view class="service-title">订酒店立减
            <text class="amount-price">{{hotelCoupon.vouShowPrice}}元</text>
            <text bindtap="showhotelDetail" class="se-circle">?</text>
          </view>
          <view class="flex-box hotel-count">¥
            <text>{{hotelCoupon.vouSellPrice}}</text>/份
            <text bindtap="hotelLessBtn" class="hotel-less">-</text>
            <text class="hotel-num">
                            <text>{{hotelCouNum}}</text>份</text>
            <text bindtap="hotelPlusBtn" class="hotel-plus">+</text>
          </view>
        </view>
        <view class="Insurance" style="order:{};" wx:if="{{!isPeaceTravel&&isAdult&&plists.length&&isHasInsurance}}">
          <view class="weatherFactor weatherFactor2 new-fbox flex-box-center" wx:if="{{weather.weatherCode}}">
            <view class="new-fbox flex-box-center wLeft" style="color:#ff6815">
              <image mode="widthFix" src="{{weather.pic}}"></image>
              <view class="city">{{weather.departure||weather.destination}}</view>
              <view class="date">{{weather.dptDate}}</view>
              <view class="i" wx:if="{{!weather.desc&&!wdocument}}">{{weather.weatherValue}}</view>
            </view>
            <view class="wRight" wx:if="{{weather.desc&&!wdocument}}">
              <view class="i">{{weather.weatherValue}}</view>
              <text>{{weather.desc}}</text>
              <view class="tip">建议您为自己和家人添一份安心</view>
            </view>
            <view class="wRight" wx:elif="{{wdocument}}">
              <view class="i">{{weather.weatherValue}}</view>
              <text>{{wdocument}}</text>
              <view class="tip">建议您为自己和家人添一份安心</view>
            </view>
          </view>
          <view bindtap="btnSelectIns" class="innItem" data-code="{{item.insCode}}" data-index="{{index}}" wx:for="{{defInsMulti}}">
            <view class="inn_txt flex_box flex_align_center">
              <view class="inn_title flex_1">
                <text>{{item.insName}}</text>
                <text catchtap="btnShowInsDetail" class="icon-detail" data-code="{{item.insCode}}" data-index="{{index}}"></text>
              </view>
              <view class="inn_radio ricon {{item.insCusPayTc>0&&item.pubID>0?'ricon-check bg-color border-color':''}}"></view>
            </view>
            <view class="inn_spend flex_box flex_align_center">
              <view class="inn_tips flex_1 inn_tips2">{{item.shortDescribe}}</view>
              <view class="inn_rmb r {{!item.insCusPayTc?'fn-hide':''}}">¥
                <text>{{item.insCusPayTc}}</text>
              </view>
            </view>
          </view>
        </view>
        <view class="isPeaceTravel {{isDownArr?'':'b-radius1'}}" wx:if="{{!!isPeaceTravel&&plists.length&&isAdult}}">
          <view bindtap="btnOpneAnxin" class="anxin flex-box bor-top">
            <text class="ser-instext">安心出行</text>
            <text class="flex1 {{isDownArr?'down-arrow':'up-arrow'}}"></text>
          </view>
          <view class="anxinWrap {{isDownArr?'':'fn-hide'}}">
            <view class="flex-box bor-top item-div" wx:if="{{isAdult&&plists.length&&defIns.pubID!=0}}">
              <view class="service-title flex2">
                <text class="amount-price">{{defIns.insQuota}}万</text>高额保障，出行首选
                <text bindtap="openInsurance" class="se-circle js-insurdetail">?</text>
              </view>
              <view class="right-box flex1">¥
                <text>{{defIns.insCusPayTc}}</text>/份
                <view bindtap="selectDefaultInsure" class="icon-radio {{defInsDb?'icon-radio-on':'icon-radio-off'}}"></view>
              </view>
            </view>
          </view>
        </view>
        <view class="x-yuan-free flex_box flex_align_center" style="order:{};" wx:if="{{plists.length&&YyData.description}}">
          <view class="img"></view>
          <view class="flex_1">
            <view class="xy_title">{{YyData.description}}</view>
            <view class="xy_subtitle">{{YyData.name}}
              <text bindtap="btnToXYPage" class="icon-detail" data-xylink="{{YyData.instruction}}"></text>￥{{YyData.price}}</view>
          </view>
          <view class="btn-box">
            <view bindtap="btnSwitchXYuan" class="btn">{{YyStatus?'已参与':'立即参与'}}</view>
          </view>
        </view>
        <view class="flex-box bor-top item-div jiudianbox" style="order:{};" wx:if="{{plists.length&&hasCarCoupon}}">
          <view class="service-title">预约用车立减
            <text class="amount-price">{{carCoupon.vouShowPrice}}元</text>
            <text bindtap="showCarDetail" class="se-circle">?</text>
          </view>
          <view class="flex-box hotel-count">¥
            <text>{{carCoupon.vouSellPrice}}</text>/份
            <text bindtap="carLessBtn" class="hotel-less">-</text>
            <text class="hotel-num">
                            <text>{{carCouNum}}</text>份</text>
            <text bindtap="carPlusBtn" class="hotel-plus">+</text>
          </view>
        </view>
        <block wx:if="{{sortList.vipService.cssStyle!=2}}">
          <template is="valueAddService" data="{{plists:plists,pkg:pkg,isAdult:isAdult,isHasOptSvc:isHasOptSvc,isHasExtSvc:isHasExtSvc,isAdult:isAdult,defaultOpt:defaultOpt,defaultExt:defaultExt,sortList:sortList}}" wx:if="{{isPeaceTravel&&isAdult&&!defInsDb}}"></template>
          <template is="valueAddService" data="{{plists:plists,pkg:pkg,isAdult:isAdult,isHasOptSvc:isHasOptSvc,isHasExtSvc:isHasExtSvc,isAdult:isAdult,defaultOpt:defaultOpt,defaultExt:defaultExt,sortList:sortList}}" wx:if="{{isPeaceTravel&&!isAdult}}"></template>
          <template is="valueAddService" data="{{plists:plists,pkg:pkg,isAdult:isAdult,isHasOptSvc:isHasOptSvc,isHasExtSvc:isHasExtSvc,isAdult:isAdult,defaultOpt:defaultOpt,defaultExt:defaultExt,sortList:sortList}}" wx:if="{{!isPeaceTravel}}"></template>
        </block>
        <view class="style2-serins" style="order:{};" wx:if="{{sortList.vipService.cssStyle==2&&!isPeaceTravel||sortList.vipService.cssStyle==2&&isPeaceTravel&&!defInsDb&&isAdult||sortList.vipService.cssStyle==2&&isPeaceTravel&&!isAdult}}">
          <view bindtap="showVipFn" class="style2-serins-title" wx:if="{{isAdult&&isHasOptSvc}}">优选服务</view>
          <view bindtap="showVipFn" class="style2-serins-title" wx:if="{{!isAdult&&isHasExtSvc}}">至尊服务</view>
          <view class="style2-serins-list" style="height:{};">
            <view bindtap="selectVips" class="style2-serins-item flex-box" data-pid="{{item.serviceId}}" wx:for="{{slectedVAList}}">
              <view class="style2-serins-item-title flex-box">
                <text class="big-title">{{item.serviceName}}</text>
                <text catchtap="vipCss2Btn" class="rdetail hottips" data-pid="{{item.serviceId}}" wx:if="{{item.sellPrice!=0}}"></text>
              </view>
              <view class="style2-serins-item-subtitle flex3 {{item.sellPrice==0?'bigPL':''}}">{{item.subTitle||''}}</view>
